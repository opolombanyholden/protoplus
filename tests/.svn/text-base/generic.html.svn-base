<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=utf8" />
		<title>Generic Animation</title>
        <script type="text/javascript" src="../prototype.js"></script>
        <script type="text/javascript" src="../protoplus.js"></script>
        <script>
            document.ready(function(){
                $("box").shift({top:200, left:200, height:200, width:200, fontSize:22, color:'#000000', duration:1, onEnd: function(){
                    $("box").shift({top:1, left:1, height:100, width:100, fontSize:11, color:'#333333', duration:1});
                }});
            })
        </script>
        <style>
            #box{
                height:100px;
                width:100px;
                border:1px solid #ccc;
                background:#f5f5f5;
                position:absolute;
                top:1px;
                left:1px;
                color:#333;
                font-size:11px;
            }
        </style>
	</head>
	<body>
	    <div id="box">
	        Text text
	    </div>
	</body>
</html>
