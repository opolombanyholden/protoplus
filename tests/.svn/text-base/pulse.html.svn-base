<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<html>
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
		<title>Pulse Color</title>
        <script type="text/javascript" src="../prototype.js"></script>
        <script type="text/javascript" src="../protoplus.js"></script>
        <script>
            function pulseme(){
                $('pulse').setStyle({
                    borderColor: '#ffaa00',
                    backgroundColor: '#0033aa',
                    color: '#aaaaaa'
                });
                
                $('pulse').shift({
                    easing:'pulse',
                    easingCustom:3,
                    duration:3,
                    borderColor: '#00aaff',
                    backgroundColor: '#aaFF00',
                    color: '#222222'
                });
            }
        </script>
        <style>
            body{
                font-family:"Trebuchet MS";
                font-weight:bold;
                
            }
        </style>
	</head>
	<body>
	    <div id="pulse" onclick="pulseme()" style="border-width:5px; border-style:solid; border-color:#ffaa00; background:#0033aa; color:#aaaaaa; padding:10px; width:250px;">
            Text Content Here, Hello World or Whatever.<br>
            Click Me to pulse
        </div>
	</body>
</html>
